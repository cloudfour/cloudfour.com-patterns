{% embed '../../../objects/rhythm/rhythm.twig' with {
  class: 'o-rhythm--condensed'
} only %}
  {% block content %}
    {% for i in -2..6 %}
      {% set class_level = i|abs %}
      {% if i < 0 %}
        {% set class_level = 'n' ~ class_level %}
      {% endif %}
      {% include '../heading.twig' with {
        level: i,
        content: 'c-heading--level-' ~ class_level
      } only %}
    {% endfor %}
  {% endblock %}
{% endembed %}
