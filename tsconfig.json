{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "declaration": true,
    "noEmit": true,
    "allowJs": true,
    "checkJs": true
  },
  "include": ["src"],
  "exclude": ["src/prototypes"]
}
