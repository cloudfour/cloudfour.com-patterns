      {% embed '@cloudfour/components/heading/heading.twig' with {
        level: -2,
      } only %}
        {% block content %}
          {# Let’s talk #}
          Hire us
        {% endblock %}
      {% endembed %}
    {% endblock %}
    {% block content %}
      <p>
        We’d love to chat about your project, answer your questions or show off what our team can do.
        {# Call our office at <a href="#">+1&nbsp;(503)&nbsp;290-1090</a> or <a href="#">email us</a> to get started. #}
      </p>
      {% embed '@cloudfour/objects/button-group/button-group.twig' with {
        grow: true
      } only %}
        {% block content %}
          {% include '@cloudfour/components/button/button.twig' with {
            label: 'Email us',
            content_start_icon: 'envelope',
            href: '#',
          } only %}
          {% include '@cloudfour/components/button/button.twig' with {
            label: '+1 (503) 290-1090',
            href: '#',
            class: 'c-button--secondary'
          } only %}
        {% endblock %}
      {% endembed %}
    {% endblock %}
