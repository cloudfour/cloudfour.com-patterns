{
  "name": "@cloudfour/patterns",
  "version": "0.1.0",
  "author": "Cloud Four",
  "description": "Front-end patterns for cloudfour.com",
  "homepage": "https://github.com/cloudfour/cloudfour.com-patterns",
  "license": "MIT",
  "keywords": [
    "pattern",
    "styleguide",
    "component",
    "framework",
    "boilerplate",
    "html",
    "ui",
    "css",
    "bem",
    "typography"
  ],
  "engines": {
    "node": ">=12.16.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudfour/cloudfour.com-patterns.git"
  },
  "main": "./dist/cloudfour-patterns.js",
  "unpkg": "./dist/cloudfour-patterns.min.js",
  "module": "./dist/cloudfour-patterns.mjs",
  "exports": "./dist/cloudfour-patterns.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist",
    "/src/**/*.scss",
    "/src/**/*.twig",
    "!/src/**/demo/**/*.twig",
    "/src/assets",
    "!.gitignore",
    "!src/index.scss"
  ],
  "dependencies": {
    "focus-visible": "5.1.0",
    "prismjs": "1.20.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.0",
    "@cloudfour/eslint-plugin": "11.0.0",
    "@cypress/browserify-preprocessor": "3.0.0",
    "@rollup/plugin-babel": "5.0.2",
    "@rollup/plugin-node-resolve": "8.0.0",
    "@storybook/addon-a11y": "5.3.19",
    "@storybook/addon-docs": "5.3.19",
    "@storybook/addon-knobs": "5.3.19",
    "@storybook/addon-viewport": "5.3.19",
    "@storybook/html": "5.3.19",
    "@svgr/webpack": "5.4.0",
    "@testing-library/cypress": "6.0.0",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "babel-loader": "8.1.0",
    "browserify-transform-tools": "1.7.0",
    "css-loader": "3.5.3",
    "cssnano": "4.1.10",
    "cypress": "4.7.0",
    "dts-bundle-generator": "4.3.0",
    "eliminate": "1.1.2",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-mdx": "1.7.0",
    "eslint-plugin-react": "7.20.0",
    "gulp": "4.0.2",
    "gulp-postcss": "8.0.0",
    "gulp-prettier": "3.0.0",
    "gulp-rename": "2.0.0",
    "gulp-sass": "4.1.0",
    "gulp-svgmin": "2.2.0",
    "gulp-theo": "2.0.1",
    "js-yaml": "3.14.0",
    "lodash": "4.17.15",
    "ltx": "2.9.2",
    "mini-css-extract-plugin": "0.9.0",
    "npm-run-all": "4.1.5",
    "postcss-inline-svg": "4.1.0",
    "postcss-loader": "3.0.0",
    "prettier": "2.0.5",
    "remark-preset-lint-recommended": "4.0.0",
    "remark-preset-prettier": "0.4.0",
    "resolve": "1.17.0",
    "rollup": "2.10.9",
    "rollup-plugin-terser": "6.1.0",
    "sass": "1.26.5",
    "sass-loader": "8.0.2",
    "storybook-addon-paddings": "2.0.2",
    "storybook-addon-themes": "5.4.1",
    "style-loader": "1.2.1",
    "stylelint": "13.5.0",
    "stylelint-config-cloudfour": "3.1.2",
    "stylelint-config-prettier": "8.0.1",
    "theo": "8.1.5",
    "through2": "3.0.1",
    "tiny-glob": "0.2.6",
    "twig": "1.15.1",
    "twigjs-loader": "1.0.1",
    "typescript": "3.9.3"
  },
  "scripts": {
    "storybook": "npm run preprocess && run-p watch-preprocess start-storybook",
    "start-storybook": "start-storybook -p 6006 -s static",
    "build-storybook": "npm run preprocess && build-storybook -s static",
    "preprocess": "run-p preprocess:*",
    "preprocess:svg-to-twig": "gulp svgToTwig",
    "preprocess:theo-to-mdx": "gulp theoToMDX",
    "watch-preprocess": "gulp watchPreprocess",
    "build:js": "gulp buildJS",
    "build:types": "gulp buildTypes",
    "build:sass": "gulp buildSass",
    "build": "npm run clean && run-p build:*",
    "clean": "eliminate dist",
    "type": "tsc",
    "type:watch": "tsc --watch",
    "check-lint": "run-s check-lint:*",
    "check-lint:prettier": "prettier . --check",
    "check-lint:css": "stylelint '**/*.scss'",
    "check-lint:js": "eslint . --ext .js,.mdx,.ts,.tsx",
    "lint": "run-s lint:*",
    "lint:prettier": "prettier . --write",
    "lint:css": "stylelint --fix '**/*.scss'",
    "lint:js": "eslint . --fix --ext .js,.mdx,.ts,.tsx",
    "test": "cypress run",
    "test:open": "cypress open",
    "prepublishOnly": "npm ci && npm run check-lint && npm run preprocess && npm run build"
  }
}
