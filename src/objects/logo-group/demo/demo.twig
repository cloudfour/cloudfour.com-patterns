{% embed '@cloudfour/objects/container/container.twig' %}
  {% block content %}
    {% embed '@cloudfour/objects/logo-group/logo-group.twig' %}
      {% block content %}
        {% for item in items %}
          {% include '@cloudfour/components/logo/logo.twig' with {
            src: item.src,
            alt: item.alt
          } only %}
        {% endfor %}
      {% endblock %}
    {% endembed %}
  {% endblock %}
{% endembed %}
