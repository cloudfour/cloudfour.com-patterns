<li id="footnote-{{count}}">
  {% block content %}{% endblock %}
  <a href="#footnote-anchor-{{count}}">
    <span class="u-hidden-visually">Return to the text before footnote {{count}}</span>
      {% include '@cloudfour/components/icon/icon.twig' with {
        name: 'arrow-hook-left',
        inline: true
      } %}
  </a>
</li>
