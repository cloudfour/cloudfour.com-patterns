{
  "name": "@cloudfour/patterns",
  "version": "0.3.2",
  "author": "Cloud Four",
  "description": "Front-end patterns for cloudfour.com",
  "homepage": "https://github.com/cloudfour/cloudfour.com-patterns",
  "license": "MIT",
  "keywords": [
    "pattern",
    "styleguide",
    "component",
    "framework",
    "boilerplate",
    "html",
    "ui",
    "css",
    "bem",
    "typography"
  ],
  "engines": {
    "node": ">=12.16.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudfour/cloudfour.com-patterns.git"
  },
  "main": "./dist/cloudfour-patterns.js",
  "unpkg": "./dist/cloudfour-patterns.min.js",
  "module": "./dist/cloudfour-patterns.mjs",
  "exports": "./dist/cloudfour-patterns.mjs",
  "types": "./dist/cloudfour-patterns.d.ts",
  "files": [
    "/dist",
    "/src/**/*.scss",
    "/src/**/*.twig",
    "!/src/**/demo/**/*.twig",
    "/src/assets",
    "!.gitignore",
    "!src/index.scss"
  ],
  "dependencies": {
    "focus-visible": "5.2.0",
    "prismjs": "1.22.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/preset-env": "7.12.1",
    "@changesets/changelog-github": "0.2.7",
    "@changesets/cli": "2.11.2",
    "@cloudfour/eslint-plugin": "15.0.1",
    "@cypress/webpack-preprocessor": "5.4.11",
    "@rollup/plugin-babel": "5.2.1",
    "@rollup/plugin-node-resolve": "10.0.0",
    "@storybook/addon-a11y": "6.0.28",
    "@storybook/addon-controls": "6.0.28",
    "@storybook/addon-docs": "6.0.28",
    "@storybook/addon-viewport": "6.0.28",
    "@storybook/html": "6.0.28",
    "@svgr/webpack": "5.4.0",
    "@testing-library/cypress": "7.0.1",
    "@types/prismjs": "1.16.2",
    "@whitespace/storybook-addon-html": "2.0.1",
    "@wordpress/block-library": "2.26.3",
    "babel-loader": "8.1.0",
    "browserify-transform-tools": "1.7.0",
    "css-loader": "4.3.0",
    "cssnano": "4.1.10",
    "cypress": "5.6.0",
    "eliminate": "1.1.2",
    "eslint": "7.13.0",
    "eslint-plugin-mdx": "1.8.2",
    "eslint-plugin-react": "7.21.5",
    "gulp": "4.0.2",
    "gulp-postcss": "8.0.0",
    "gulp-prettier": "3.0.0",
    "gulp-rename": "2.0.0",
    "gulp-sass": "4.1.0",
    "gulp-svgmin": "3.0.0",
    "gulp-theo": "2.0.1",
    "html-to-react": "1.4.5",
    "js-yaml": "3.14.0",
    "lodash": "4.17.20",
    "ltx": "2.10.0",
    "mini-css-extract-plugin": "1.2.1",
    "npm-run-all": "4.1.5",
    "postcss-inline-svg": "4.1.0",
    "postcss-loader": "3.0.0",
    "prettier": "2.1.2",
    "raw-loader": "4.0.2",
    "remark-preset-lint-recommended": "4.0.1",
    "remark-preset-prettier": "0.4.0",
    "resolve": "1.18.1",
    "rollup": "2.33.1",
    "rollup-plugin-dts": "1.4.13",
    "rollup-plugin-terser": "7.0.2",
    "sass": "1.29.0",
    "sass-loader": "10.0.5",
    "storybook-addon-paddings": "2.0.2",
    "storybook-addon-themes": "6.0.0",
    "style-loader": "2.0.0",
    "stylelint": "13.7.2",
    "stylelint-config-cloudfour": "3.1.2",
    "stylelint-config-prettier": "8.0.2",
    "theo": "8.1.5",
    "through2": "4.0.2",
    "tiny-glob": "0.2.6",
    "twing": "3.1.1",
    "twing-loader": "3.0.1",
    "typescript": "4.0.5",
    "val-loader": "2.1.2",
    "webpack": "4.44.2"
  },
  "scripts": {
    "storybook": "npm run preprocess && run-p watch-preprocess start-storybook",
    "start-storybook": "start-storybook -p 6006 -s static",
    "build-storybook": "npm run preprocess && build-storybook -s static",
    "preprocess": "run-p preprocess:*",
    "preprocess:svg-to-twig": "gulp svgToTwig",
    "preprocess:theo": "gulp theo",
    "watch-preprocess": "gulp watchPreprocess",
    "build:js": "gulp buildJS",
    "build:types": "npm run type && gulp buildTypes",
    "build:sass": "gulp buildSass",
    "build": "run-s clean preprocess && run-p build:*",
    "clean": "eliminate dist",
    "type": "tsc",
    "type:watch": "tsc --watch",
    "check-lint": "run-s check-lint:*",
    "check-lint:prettier": "prettier . --check",
    "check-lint:css": "stylelint '**/*.scss'",
    "check-lint:js": "eslint .",
    "lint": "run-s lint:*",
    "lint:prettier": "prettier . --write",
    "lint:css": "stylelint --fix '**/*.scss'",
    "lint:js": "eslint . --fix",
    "test": "cypress run",
    "test:open": "cypress open",
    "version": "changeset version && prettier --write .",
    "release": "npm run build && changeset publish"
  }
}
