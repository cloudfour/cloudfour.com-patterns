<div class="o-individual-bio-group">
  <div class="o-individual-bio-group__bio_image">
    {% block image %}{% endblock %}
  </div>
  {% if links is not empty %}
    <div class="o-individual-bio-group__links">
      <ul>
      {% for link in links %}
        <li>
          <a href="{{ link.href }}">{{ link.text }}</a>
        </li>
      {% endfor %}
      </ul>
    </div>
  {% endif %}
  <div class="o-individual-bio-group__content">
    {% block content %}{% endblock %}
  </div>
</div>
